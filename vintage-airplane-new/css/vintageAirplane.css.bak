/*****************************************************/
/******************** Upper Nav Bar*******************/
/*****************************************************/
.upper_nav {
	height: 31px;
	background: #18343E;
	padding: 0px;
}
                                                               
.upper_nav ul {
	list-style-type: none; /* this setting gets rid of the bullets */
	/*background: #18343E;*/
	display: inline;	
	padding-left: 145px;
	
}

.upper_nav ul li {
	 background: #18343E;
	/*float: left; /* this setting makes it a horizontal menu */
	display: inline;
	/* font: 14px/30px "Arial Black", Gadget, sans-serif; */
	color: #FFF;
	text-decoration: none;
	padding: 9px 55px 7px;
	border-right: 2px solid #18343E;
	border-left: 1px solid #18343E;
}

.upper_nav ul li a {
	font: 14px/30px "Arial Black", Gadget, sans-serif;
	color: #FFF;
	text-decoration: none;
}

.upper_nav ul li a:link, .upper_nav ul li a:visited {
	
}

.upper_nav ul li:hover, .upper_nav ul li:active {
	background: #470606;
	height: 30px;
	line-height: 30px;
	border-right: 2px solid #933939;
	border-left: 1px solid #930C0C;
	
}
	
	
	
#picture_backgnd {
	background: url(../images/decoC45_960x640-20pcnt.png);
	height: 510px;
	width: 950px;
	
}


